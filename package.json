{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "commit": "git add . && git-cz --disable-emoji",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js}": [
      "bash -c tsc",
      "yarn lint",
      "yarn format",
      "git add ."
    ]
  },
  "dependencies": {
    "@next/font": "13.1.6",
    "@types/node": "18.11.19",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "next": "13.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.1"
  }
}
